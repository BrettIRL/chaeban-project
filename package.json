{
  "name": "chaeban-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:tailwind": "tailwind build src/styles/tailwind.css -o src/styles/tailwind.output.css",
    "watch:tailwind": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/styles/tailwind.output.css -c 'npm run build:tailwind'",
    "dev": "npm-run-all build:tailwind --parallel watch:tailwind start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "autoprefixer": "^10.0.2",
    "classnames": "^2.2.6",
    "formik": "^2.2.5",
    "moment": "^2.29.1",
    "postcss": "^8.1.10",
    "react": "^17.0.1",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.1",
    "react-flatpickr": "^3.10.6",
    "react-icons": "^4.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "tailwindcss": "^2.0.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4",
    "yup": "^0.31.0"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/classnames": "^2.2.11",
    "@types/moment": "^2.13.0",
    "@types/react-color": "^3.0.4",
    "@types/react-flatpickr": "^3.8.1",
    "@types/react-icons": "^3.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/yup": "^0.29.9",
    "chokidar-cli": "^2.1.0",
    "npm-run-all": "^4.1.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
